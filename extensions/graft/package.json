  "devDependencies": {
    "@types/bun": "latest",
    "@types/chrome": "^0.0.287",
    "oxfmt": "^0.21.0",
    "oxlint": "^1.36.0",
    "sharp": "^0.34.5",
    "typescript": "^5"
  }
}
